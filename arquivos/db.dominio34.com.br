@	IN	A	192.168.34.10
;@	IN	AAAA	::1
servidor1	IN	A	192.168.34.10
dns1	IN	CNAME	servidor1.dominio34.com.br.
dns2	IN	A	192.168.34.20
cliente	IN	A	192.168.34.30
